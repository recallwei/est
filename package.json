{
  "private": true,
  "name": "est",
  "version": "0.0.1",
  "description": "EST (Express Starter Template) is an opinionated Express/TypeScript/Prisma/PostgreSQL starter template.",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "keywords": [
    "node.js",
    "express",
    "typescript",
    "prisma"
  ],
  "homepage": "https://github.com/recallwei/est#readme",
  "repository": "github:recallwei/est",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "pnpm clear && tsc --build",
    "start": "cross-env NODE_ENV=production npm run build && ts-node ./dist/src/index.js",
    "clear": "tsc --build --clean",
    "prepare": "husky install",
    "cspell:check": "cspell **",
    "cspell:sort": "node ./scripts/cspell-sort.js",
    "eslint:check": "eslint . --ext .js,.ts --color",
    "eslint:fix": "pnpm check:lint -- --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --check --write --ignore-unknown .",
    "type:check": "tsc --pretty --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format"
  },
  "dependencies": {
    "@ngneat/falso": "^6.4.0",
    "@node-rs/bcrypt": "^1.7.1",
    "@prisma/client": "^4.15.0",
    "body-parser": "^1.20.2",
    "chalk": "4.1.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.1.1",
    "express": "~4.18.2",
    "figlet": "^1.6.0",
    "gradient-string": "^2.0.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.10.0",
    "multer": "1.4.5-lts.1",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "4.17.17",
    "@types/figlet": "^1.5.6",
    "@types/gradient-string": "^1.1.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "20.2.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "cspell": "^6.31.1",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-prisma": "^4.13.0",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "packageManager": "pnpm@8.5.0",
  "engines": {
    "node": ">=16.14.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "ts-node src/prisma/seed.ts"
  },
  "license": "MIT"
}
