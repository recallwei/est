{
  "private": true,
  "name": "est",
  "version": "1.0.0",
  "description": "EST (Express Starter Template) is an opinionated Express/TypeScript/Prisma/PostgreSQL starter template.",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "keywords": [
    "Node.js",
    "Express",
    "TypeScript",
    "Prisma"
  ],
  "homepage": "https://github.com/recallwei/est#readme",
  "repository": "github:recallwei/est",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "pnpm clear && tsc --build",
    "start": "cross-env NODE_ENV=production npm run build && node ./dist/index.js",
    "clear": "tsc --build --clean",
    "check:spelling": "cspell **",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --ext .js,.ts --color",
    "fix:format": "prettier --check --write --ignore-unknown .",
    "fix:lint": "pnpm check:lint -- --fix",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ngneat/falso": "^6.4.0",
    "@node-rs/bcrypt": "^1.7.1",
    "@prisma/client": "^4.14.1",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "~4.18.2",
    "http-errors": "~2.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.10.0",
    "pg": "^8.11.0",
    "prisma": "^4.14.1",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.8",
    "@types/express": "4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "20.2.3",
    "@types/pg": "^8.10.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "cspell": "^6.31.1",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-prisma": "^4.13.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "packageManager": "pnpm@8.5.0",
  "engines": {
    "node": ">=16.14.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "ts-node src/prisma/seed.ts"
  },
  "license": "MIT"
}
